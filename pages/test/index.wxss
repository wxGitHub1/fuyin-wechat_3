/**悬浮按钮***/
.fixed{
  position: fixed;
  bottom: 20rpx;
  right: 20rpx;
  /* background: rgb(189, 205, 209); */
}
.fixed .fixed-1,.fixed .fixed-2{
  background:#85BEF9;
  padding: 20rpx;
  width: 80rpx;
  height: 80rpx;
  text-align: center;
  border-radius: 50%;
  color: #fff;
  font-size: 25rpx;
}
.fixed .fixed-1 view,.fixed .fixed-2 view{
  line-height: 40rpx;
}
.fixed .fixed-2{
  margin-top: 20rpx;
  background: #FCB830;
}
