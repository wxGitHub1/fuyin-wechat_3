/* pages/index/home.wxss */
swiper {
  height: 100%;

}

.swiper-container {
  position: relative;
}

.swiper-container .swiper {
  /* height: 100rpx; */
  position: relative;
}

.swiper-container .longin-page {
  width: 100%;
  position: relative;
  top: 0;
  background: #fff;
  box-sizing: border-box;
  padding-top: 1rpx;
}

.swiper-container .longin-page .logoBgImg {
  width: 100%;
  height: 100%;
  background: url(https://fuyinkangfu.com:8085/files/wx/1-1.jpg) no-repeat;
  background-size: 100% 100%;
}

.swiper-container .longin-page .logoBgImg2 {
  width: 100%;
  height: 100%;
  background: url(https://fuyinkangfu.com:8085/files/wx/1.jpg) no-repeat;
  background-size: 100% 100%;
}


.swiper-container .swiper .prompt-top {
  position: absolute;
  box-sizing: border-box;
  width: 100%;
  height: 56rpx;
  bottom: 0;
  text-align: center;
}

.swiper-container .swiper .prompt-top-image {
  height: 50rpx;
  width: 60rpx;
  animation: myfirst 1s linear infinite;
  position: relative;
}

@keyframes myfirst {
  from {
    bottom: 0;
  }

  to {
    bottom: 30rpx;
  }
}

